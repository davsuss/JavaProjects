package GameOfLife;

public class Cell {

private boolean status;
private boolean nextState;
private boolean changed;
Cell()
{
status = false; 
nextState = true; 
changed = false;
}


public boolean GetStatus() 
{ 
return status;	
}
public void SetNextState(boolean nextState)
{
changed = true;
nextstate
}
public void TickGeneration() 
{
if(changed)
	status = nextState;
changed = false;
return;
}



	
}
