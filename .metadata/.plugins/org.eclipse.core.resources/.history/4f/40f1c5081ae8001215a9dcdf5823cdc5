package GameOfLife;
import java.awt.*;
import javax.swing.*;
import java.awt.event.*;
public class Frame extends JFrame implements ActionListener {


Frame(){
setTitle("Test");	
setSize(200,200);
setResizable(false);
Container framecontainer = getContentPane();
framecontainer.add(new ButtonPanel(this));
}

@Override
public void actionPerformed(ActionEvent e) {
	System.out.println("Event Heard At" + System.currentTimeMillis() );
	
}
	
	
}
