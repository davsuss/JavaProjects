package main;

public class JCell {
	
private Boolean m_alive;
private Boolean m_changed;
private Boolean m_nextState;
public JCell()
{
m_alive = m_changed = m_nextState = false;
}

public Boolean isAlive()
{
return m_alive;
}
public void SetState(Boolean nextState)
{
m_changed = true;
m_alive = nextState;
}
public void Itterate()
{

}



}
