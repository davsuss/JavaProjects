package main;
import javax.swing.*;
import java.awt.*;
public class GameBoard extends JComponent{
	
public void paint(Graphics g)
{ 
Boolean colorblack = true;
for(int y = 0; y < 10;y++)
{ 
for(int x = 0; x < 10;x++ )
{
if(colorblack)
{
g.setColor(Color.BLACK);
}
else 
{ 
g.setColor(Color.WHITE);
}
colorblack = !colorblack;	
g.fillRect(50*(x),0,50,50);
}
colorblack = !colorblack;
}



}
	
	
	
}
